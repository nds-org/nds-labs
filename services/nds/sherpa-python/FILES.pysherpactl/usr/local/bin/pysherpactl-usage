#!/bin/sh
cat <<EOF

Usage:
    pysherpactl DVOL <data-URI> DPATH <PATH> XFRCMD <transfer-cmd> <docker cmd>

Ex.
    pysherpactl DVOL mydata DPATH /data DSRC https://github.com/mbtaviz/mbtaviz.github.io/archive/master.zip DXFRCMD 'cd ${DPATH} && wget ${DSRC} && unzip master.zip' jupyter-scipy-notebook


EOF


